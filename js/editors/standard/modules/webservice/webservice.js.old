function WebService(){

}

WebService.create = function(type){
    switch(type){
        case 'php':
            return new PhpWebservice();
            break;
        case 'node':
            return new NodeWebservice();
            break;
        case 'fake':
            return new FakeWebservice();
            break;

        default :
            return new FakeWebservice();
            break;
    }
}



